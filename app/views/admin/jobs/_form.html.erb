<%= simple_form_for [:admin, @job] do |f| %>
<%= f.input :title %>
<%= f.input :description %>
<%= f.input :wage_lower_bound, :Label => "salary lower bound" %>
<%= f.input :wage_upper_bound, :Label => "salary upper bound" %>
<%= f.input :contact_email %>
<%= f.input :is_hidden %>
<%= f.submit "Submit" %>
<% end %>
